<?php

namespace App\Tests;
use PHPUnit\Framework\TestCase;
class MethodCallCollectorTest extends TestCase
{


    protected function setUp() :void {

        exec("../phpstan-src/bin/phpstan  analyse ./tests/testCases/ObjectTestCase.php");
    }


    public function testProcessNodeMethodCallForSpecificObjectType()
    {       
        $expectedOutput = "xyz::dontcallme => Employee::raiseSalary\n";
        $this->assertStringEqualsFile('./func-calls.txt', $expectedOutput);
    }
}
