parameters:
    level: 6
    polluteScopeWithLoopInitialAssignments: false
    checkFunctionNameCase: false
    excludePaths:
        analyse:
            - vendor
            - node_modules
            - phpstan-src
rules:
    - App\MeraRule 
    - App\MethodCallRule
    

services:
    -
        class: App\MeraCollector
        tags:
            - phpstan.collector
    -
        class: App\MethodCallCollector
        tags:
            - phpstan.collector
includes:
    - phpstan-baseline.neon